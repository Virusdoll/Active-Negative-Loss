{
    "model": {
        "name": "toymodel8l"
    },
    "loss": {
        "name": "anl_ce_er",
        "alpha": 5.0,
        "beta": 5.0,
        "delta": 5e-5,
        "lamb": 2,

        "min_prob": 1e-7
    },
    "dataset": {
        "name": "cifar10",
        "num_classes": 10,
        "train_batchsize": 128,
        "test_batchsize": 256,
        "num_workers": 4
    },
    "optim": {
        "total_epoch": 120,
        
        "optimizer": "sgd",
        "learning_rate": 0.01,
        "momentum": 0.9,
        "weight_decay": 0,

        "grad_bound": 5.0,
        
        "scheduler": "cosine",
        "T_max": 120,
        "eta_min": 0
    }
}